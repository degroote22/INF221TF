{
  "name": "prisma",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "start": "dotenv -- nodemon -e ts,graphql -x ts-node src/index.ts",
    "dev": "npm-run-all --parallel start playground",
    "debug": "dotenv -- nodemon -e ts,graphql -x ts-node --inspect src/index.ts",
    "playground": "graphql playground",
    "build": "rimraf dist && tsc",
    "app": "CODEGEN_ENUMS_AS_TYPES=TRUE gql-gen --require ts-node/register --template typescript --export ./scripts/importSchema.ts --out ./src/generated/"
  },
  "dependencies": {
    "@types/node": "^10.3.6",
    "dotenv-cli": "1.4.0",
    "graphql-cli": "2.16.1",
    "graphql-code-generator": "^0.9.1",
    "graphql-codegen-typescript-template": "^0.9.1",
    "graphql-import": "^0.6.0",
    "prisma": "^1.10.2",
    "prisma-binding": "^2.1.0",
    "ts-node": "^7.0.0",
    "typescript": "^2.9.2",
    "@types/jsonwebtoken": "^7.2.7",
    "bcryptjs": "2.4.3",
    "graphql-yoga": "1.14.6",
    "io-ts": "^1.1.3",
    "jsonwebtoken": "8.2.2",
    "@types/bcryptjs": "2.4.1",
    "nodemon": "1.17.5",
    "npm-run-all": "4.1.3",
    "rimraf": "2.6.2"
  }
}
